			0,
         cast (envios.nro_nota as varchar(50)), 
         envios.Fecha_envio,
         envios.Fecha_envio,
         Monedas.sigla,  
         dep_d.Nombre_deposito, 
         suc_o.nombre_sucursal,
/*NOMBRE_PROVEEDOR*/
			isnull(sum(dbo.Cambiar2(  productos.precio_costo_mon, Par_sys.cod_moneda, envios.FECHA_envio,
							case productos.tipo_fiscal
							when 0 then
								case productos.imp_imp
								 	when 5 then
										(((envios_det.cantidad * envios_det.costo) * productos.imp_imp) / 100)
									else
										0
								end
							when 1 then 
								0 
							end, 0 )),0) AS IMPORTE_IMPUESTO_5,

			isnull(sum(dbo.Cambiar2(  productos.precio_costo_mon, Par_sys.cod_moneda, envios.FECHA_envio,
						case productos.tipo_fiscal
							when 0 then
								case productos.imp_imp
								 	when 10 then
								 		(((envios_det.cantidad * envios_det.costo) * productos.imp_imp) / 100)
									when 2 then
								 		(((envios_det.cantidad * envios_det.costo) * productos.imp_imp) / 100)
								end
							when 1 then 
								0 
							end	, 0 )),0) AS IMPORTE_IMPUESTO_10,

			sum(dbo.Cambiar1(  productos.precio_costo_mon, Par_sys.cod_moneda, envios.FECHA_envio, envios_det.cantidad * envios_det.costo, 0 )) AS tot,
		  0 as importe_gravado_5,
