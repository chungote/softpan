			( dep_d.cod_sucursal = :v_suc) and
			( dep_d.Cod_deposito = :V_CodDep  OR  :V_CodDep = 0 ) and
         ( envios_DET.CODIGO = PRODUCTOS.CODIGO ) and
			( par_sys.cod_moneda = monedas.cod_moneda ) and
			( envios.FECHA_envio >= :v_fecini ) and
			( envios.FECHA_envio <= :v_fecfin ) and
			( :v_envios = 1) and ((:v_tipo = 3) or (:v_tipo = 4) or (:v_tipo = 0))
group by envios.nro_nota,
         envios.Fecha_envio,
         envios.Fecha_envio,
         Monedas.sigla,   
         dep_d.Nombre_deposito, 
         suc_o.nombre_sucursal,
