																	end
															else
																0
															end
										end
							ELSE
								0
											
							end),0) as importe_gravado_5,

			isnull(SUM(case tipo_documentos.calc_imp
							when 1 then 
										case compras_det.tipo_fiscal
												when 0 then
													case compras.iva_incluido
														when 0 then
															case compras_det.imp_imp
																when 10 then 																	
																	  dbo.Cambiar_moneda1 (Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))) 
																when 2 then 
																	  dbo.Cambiar_moneda1 (Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))) 
																end

														 else
																case compras_det.imp_imp
																	when 10 then 																		
																		dbo.Cambiar_moneda1 (Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))/1.1) 
																	when 2 then 																
																		dbo.Cambiar_moneda1 (Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))/1.02) 
																	end
														end
				 								else
														0
												end
										else
												0															
				 			end),0) as importe_gravado_10,

			isnull(SUM( case tipo_documentos.calc_imp
							when 1 then 
									case compras_det.tipo_fiscal
										when 1 then
											dbo.Cambiar_moneda1 (  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  ((Compras_det.Cantidad + Compras_det.cant_desc) * Compras_det.Precio_bruto)) 
										else
											0
										end
							else
									dbo.Cambiar_moneda1 (  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, 
																			Case compras.iva_incluido
																				when 1 then
																			 		(((Compras_det.Cantidad + Compras_det.cant_desc) * Compras_det.Precio_bruto) - (((Compras_det.Cantidad + Compras_det.cant_desc) * Compras_det.Precio_bruto) * compras_det.descuento/100))/ (1 +(compras_det.imp_imp/100))
																			   else
																			 		(((Compras_det.Cantidad + Compras_det.cant_desc) * Compras_det.Precio_bruto) - (((Compras_det.Cantidad + Compras_det.cant_desc) * Compras_det.Precio_bruto) * compras_det.descuento/100))
																				end ) 
							end ),0) as importe_exento,

			compras.nro_concepto as concepto,			
			compras.Fecha_alta,
			compras.iva_incluido
    FROM dbo.compras,
			dbo.compras_det,
         dbo.Tipo_documentos,
         dbo.Depositos,
         dbo.Monedas,
         dbo.Proveedores,
         dbo.Par_sys  
   WHERE ( Compras.nro_reg = compras_det.nro_reg ) and ( Tipo_documentos.tipo_nc = 0 ) AND  ( Tipo_documentos.snc = 0 ) and 
			( Compras.Tipo_documen = Tipo_documentos.Tipo_documen ) and  
         ( Compras.Cod_deposito = Depositos.Cod_deposito ) and  
         ( Compras.Cod_moneda = Monedas.Cod_moneda ) and  
         ( Compras.Cod_proveedor = Proveedores.Cod_proveedor ) AND
			(compras.nro_concepto = :v_nro_concepto or :v_nro_concepto is null) and
			( Depositos.cod_sucursal = :v_suc or :v_suc = 0 ) and
			(pedido_activo=0) and
        (( Compras.TIPO_COMPRA =  :v_tipo) or (:v_tipo = 0)) AND  
        (( Compras.Cod_deposito = :V_CodDep ) OR ( :V_CodDep = 0 )) AND  
        (( Compras.Cod_proveedor = :V_CodProv ) OR ( :V_CodProv = 0 )) AND  
         (( Compras.Fecha >= :V_FecIni or :V_FecIni is null) AND  
         ( Compras.Fecha <= :V_FecFin or :V_FecFin is null  )) and
			(( Compras.Fecha_factura >= :V_FecIni1 or :V_FecIni1 is null) AND  
         ( Compras.Fecha_factura <= :V_FecFin1 or :V_FecFin1 is null  )) and
			(( Compras.Fecha_alta >= :V_FecIni2 or :V_FecIni2 is null) AND  
         ( Compras.Fecha_alta < dateadd(day,1,:V_FecFin2) or :V_FecFin2 is null  )) and  
			((:v_tipo <> 1) and (:v_tipo <> 2))
GROUP BY Compras.Nro_reg,
         Tipo_documentos.Tipo_descripcion,
			Tipo_documentos.tipo_nc,
			tipo_documentos.calc_imp,
         Compras.Nro_factura,
         Compras.Fecha,
         Compras.Fecha_factura,
         Depositos.Nombre_deposito,
         Compras.Cod_moneda,
         Monedas.Sigla,
			Proveedores.Nombre_proveedor,
			compras.nro_concepto,
			compras.Fecha_alta,
			compras.iva_incluido

union all


