 retrieve=" SELECT  Compras.Nro_reg,   
         Tipo_documentos.Tipo_descripcion, 
			Tipo_documentos.tipo_nc, 
         Compras.Nro_factura,   
         Compras.Fecha,  
         Compras.Fecha_factura,
         Monedas.sigla,
         Depositos.Nombre_deposito,   
			Proveedores.Nombre_proveedor,
			SUM( dbo.Cambiar_moneda1 (  Compras.Cod_moneda,	Par_sys.cod_moneda,	Compras.Fecha,	valor) ) as Compra,   
			case tipo_documentos.calc_imp
					when 1 then 
							isnull(sum(case compras_det.tipo_fiscal
									when 0 then
										case compras.iva_incluido
											when 0 then
													case compras_det.imp_imp
															when 5 then
																dbo.Cambiar_moneda2(  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, (((((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100)) * compras_det.imp_imp) / 100) )
													else
															0
													end
											else
													case compras_det.imp_imp
															when 5 then
																dbo.Cambiar_moneda2(  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100)) - (((((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))/1.05))  )
															else
																0
															end
											end
									else
										0
					  				end),0)
					when 0 then 
								0 
			end AS Impuesto_5,
				case tipo_documentos.calc_imp
					when 1 then 
						isnull(sum(case compras_det.tipo_fiscal
								when 0 then
								  	case compras.iva_incluido
										when 0 then
											case compras_det.imp_imp
												when 10 then
													dbo.Cambiar_moneda2(  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, (((((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100)) * compras_det.imp_imp) / 100))
													/*dbo.Cambiar_moneda2(  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, round(((( (compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) * compras_det.imp_imp) / 100),0) )*/
												when 2 then
													dbo.Cambiar_moneda2(  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, (((((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100)) * compras_det.imp_imp) / 100) )
													/*dbo.Cambiar_moneda2(  Compras.Cod_moneda,Par_sys.cod_moneda, Compras.Fecha, round(((( (compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) * compras_det.imp_imp) / 100),0) )*/
												end
										else
											case compras_det.imp_imp
												when 10 then
													dbo.Cambiar_moneda2(  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100)) - (((((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))/1.1)) )
												when 2 then
													dbo.Cambiar_moneda2(  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100)) - (((((compras_det.cantidad + compras_det.cant_desc ) * compras_det.Precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))/1.02)) )
												end
										end

								else
									0
							end),0) 
					when 0 then 
								0 
			end AS Impuesto_10,

			isnull(SUM( case tipo_documentos.calc_imp
							when 1 then 
								   case compras.iva_incluido
										when 0 then
												case compras_det.tipo_fiscal
													when 0 then
														case compras_det.imp_imp
															when 5 then
																/*dbo.Cambiar_moneda1 (  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  round(((Compras_det.Cantidad + Compras_det.cant_desc) * Compras_det.Precio_bruto),0)) */
																dbo.Cambiar_moneda1 (Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))) 
															else
																0
															end
													else
														  0
													end
										else
														case compras_det.tipo_fiscal
															when 0 then
																case compras_det.imp_imp
																	when 5 then
																		dbo.Cambiar_moneda1 (Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  (((compras_det.cantidad + compras_det.cant_desc ) * compras_det.precio_bruto) - ((compras_det.cantidad * compras_det.Precio_bruto)* compras_det.descuento/100))/1.05) 
																	else
