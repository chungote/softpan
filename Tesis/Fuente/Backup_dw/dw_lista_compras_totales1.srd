			SUM( dbo.Cambiar_moneda1 (  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, round(importe_gravado_5,0)) ) as importe_gravado_5,
			SUM( dbo.Cambiar_moneda1 (  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha, round(importe_gravado_10,0)) ) as importe_gravado_10,
			SUM( dbo.Cambiar_moneda1 (  Compras.Cod_moneda, Par_sys.cod_moneda, Compras.Fecha,  round(importe_exento,0)) ) as importe_exento,
			compras.nro_concepto as concepto,
			compras.Fecha_alta,
			compras.iva_incluido        
        FROM dbo.compras,   
         dbo.Tipo_documentos, 
         dbo.Depositos,   
         dbo.Monedas,   
         dbo.Proveedores,
         dbo.Par_sys  
   
        
        WHERE ( Compras.Tipo_documen = Tipo_documentos.Tipo_documen ) AND
         ( Compras.Cod_deposito = Depositos.Cod_deposito ) and  
         ( Compras.Cod_moneda = Monedas.Cod_moneda ) and  
         ( Compras.Cod_proveedor = Proveedores.Cod_proveedor ) AND
			(compras.nro_concepto = :v_nro_concepto or :v_nro_concepto is null) and
			( Depositos.cod_sucursal = :v_suc or :v_suc = 0 ) and
        ((( Compras.TIPO_COMPRA =  :v_tipo) or (:v_tipo = 0))and (( Compras.TIPO_COMPRA <> 3) and ( Compras.TIPO_COMPRA <> 4))) AND  
        (( Compras.Cod_deposito = :V_CodDep ) OR ( :V_CodDep = 0 )) AND  
        (( Compras.Cod_proveedor = :V_CodProv ) OR ( :V_CodProv = 0 )) AND  
         (( Compras.Fecha >= :V_FecIni or :V_FecIni is null) AND  
         ( Compras.Fecha <= :V_FecFin or :V_FecFin is null  )) and
			(( Compras.Fecha_factura >= :V_FecIni1 or :V_FecIni1 is null) AND  
         ( Compras.Fecha_factura <= :V_FecFin1 or :V_FecFin1 is null  )) and
			(( Compras.Fecha_alta >= :V_FecIni2 or :V_FecIni2 is null) AND  
         ( Compras.Fecha_alta <= dateadd(day,1,:V_FecFin2) or :V_FecFin2 is null  ))
        
        GROUP BY Compras.Nro_reg,
         Tipo_documentos.Tipo_descripcion,
			Tipo_documentos.tipo_nc,
         Compras.Nro_factura,
         Compras.Fecha,
         Compras.Fecha_factura,
         Depositos.Nombre_deposito,
         Compras.Cod_moneda,
         Monedas.Sigla,
			Proveedores.Nombre_proveedor,
			compras.nro_concepto,
			compras.Fecha_alta,
			compras.iva_incluido
union all

  SELECT envios.nro_nota,   
