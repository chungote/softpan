 column=(type=char(14) updatewhereclause=yes name=pedidos_det_codigo dbname="PEDIDOS_DET.Codigo" )
 column=(type=char(50) updatewhereclause=yes name=productos_descripcion_producto dbname="PRODUCTOS.Descripcion_producto" )
 column=(type=char(14) updatewhereclause=yes name=productos_codigo_barras dbname="codigo_barras" )
 column=(type=long updatewhereclause=yes name=productos_tipo_fiscal dbname="PRODUCTOS.tipo_fiscal" )
 column=(type=decimal(3) updatewhereclause=yes name=productos_imp_imp dbname="PRODUCTOS.imp_imp" )
 column=(type=decimal(3) updatewhereclause=yes name=pedidos_det_imp_imp dbname="PEDIDOS_DET.imp_imp" )
 column=(type=decimal(5) updatewhereclause=yes name=pedidos_det_cantidad dbname="Cantidad" )
 column=(type=decimal(18) updatewhereclause=yes name=pedidos_det_precio dbname="det_Precio" )
 column=(type=char(100) updatewhereclause=yes name=pedidos_det_observacion dbname="PEDIDOS_DET.Observacion" )
 column=(type=char(4) updatewhereclause=yes name=productos_unidad dbname="PRODUCTOS.Unidad" )
 column=(type=datetime updatewhereclause=yes name=fecha_rec dbname="fecha_rec" )
 column=(type=decimal(5) updatewhereclause=yes name=cant_recibida dbname="Cant_recibida" )
 column=(type=decimal(5) updatewhereclause=yes name=cant_recibida_bonif dbname="Cant_recibida_bonif" )
 column=(type=decimal(2) updatewhereclause=yes name=cant_facturada dbname="Cant_facturada" )
 column=(type=decimal(2) updatewhereclause=yes name=cant_ajustada dbname="Cant_ajustada" )
 column=(type=decimal(2) updatewhereclause=yes name=cant_bonif dbname="Cant_bonif" )
 column=(type=decimal(7) updatewhereclause=yes name=importe_facturada dbname="Importe_facturada" )
 column=(type=decimal(7) updatewhereclause=yes name=importe_ajustado dbname="Importe_ajustado" )
 column=(type=long updatewhereclause=yes name=deposito dbname="Deposito" )
 column=(type=long updatewhereclause=yes name=oc_abierta dbname="OC.abierta" )
 column=(type=char(25) updatewhereclause=yes name=plazos_plazo dbname="PLAZOS.plazo" )
 column=(type=char(30) updatewhereclause=yes name=compras_condicion_condicion dbname="COMPRAS_CONDICION.condicion" )
 column=(type=char(14) updatewhereclause=yes name=par_sys_codigo_devol dbname="PAR_SYS.codigo_devol" )
 column=(type=long updatewhereclause=yes name=pedidos_cod_proveedor dbname="PEDIDOS.Cod_proveedor" )
 column=(type=char(11) updatewhereclause=yes name=proveedores_ruc dbname="PROVEEDORES.ruc" )
 column=(type=char(30) updatewhereclause=yes name=timbrado dbname="timbrado" )
 column=(type=datetime updatewhereclause=yes name=vto_timbrado dbname="VTO_TIMBRADO" )
 retrieve="   SELECT Pedidos.Nro_pedido,   
			Pedidos.Nro_OC,   
         Pedidos.Fecha,  
         Proveedores.Nombre_proveedor,   
         Pedidos.Observacion,   
         Pedidos_det.Codigo,   
         Productos.Descripcion_producto,   
			isnull((select top 1 codigo_alternativo from productos_codigos with (nolock)
			where codigo = Pedidos_det.Codigo), Productos.codigo_barras) as codigo_barras, 
         Productos.tipo_fiscal, 
         Productos.imp_imp,
			Pedidos_det.imp_imp,
         isnull((select sum(cantidad_ped) from RECEPCION_PRODUCTO_DET with (nolock)
			 where RECEPCION_PRODUCTO_DET.NRO_PEDIDO = Pedidos.Nro_OC and 
					 (RECEPCION_PRODUCTO_det.NRO_reg = :v_recep or :v_recep = 0) and
					 RECEPCION_PRODUCTO_DET.CODIGO = Pedidos_det.Codigo ), 0) AS Cantidad,   

   CASE OC.Abierta
   	  WHEN 2 THEN 
			isnull((select top 1 precio from RECEPCION_PRODUCTO_DET with (nolock)
			 where RECEPCION_PRODUCTO_DET.NRO_PEDIDO = Pedidos.Nro_OC and 
					 (RECEPCION_PRODUCTO_det.NRO_reg = :v_recep or :v_recep = 0) and
					 RECEPCION_PRODUCTO_DET.CODIGO = Pedidos_det.Codigo), 0) 
        ELSE
      	round(CASE tipo_documentos.calc_imp 
         	WHEN 1 THEN 
      			CASE tipo_documentos.tributo_unico 
         			WHEN 0 THEN 
								Pedidos_det.Precio / (productos.imp_imp / 100 + 1)
         			ELSE Pedidos_det.Precio
      			END
         	ELSE Pedidos_det.Precio
      	END, 2) 
			END
			as det_Precio,

         Pedidos_det.Observacion,   
         Productos.Unidad,
 
			(select max(RECEPCION_PRODUCTO.fecha) from RECEPCION_PRODUCTO with (nolock)
			 where RECEPCION_PRODUCTO.NRO_PEDIDO = Pedidos.Nro_OC and
					 (RECEPCION_PRODUCTO.NRO_reg = :v_recep or :v_recep = 0)) as fecha_rec,

			isnull((select sum(cantidad_rec) from RECEPCION_PRODUCTO_DET with (nolock)
			 where RECEPCION_PRODUCTO_DET.NRO_PEDIDO = Pedidos.Nro_OC and 
					 (RECEPCION_PRODUCTO_det.NRO_reg = :v_recep or :v_recep = 0) and
					 RECEPCION_PRODUCTO_DET.CODIGO = Pedidos_det.Codigo ), 0) AS Cant_recibida,

			isnull((select sum(cant_bonif) from RECEPCION_PRODUCTO_DET with (nolock)
			 where RECEPCION_PRODUCTO_DET.NRO_PEDIDO = Pedidos.Nro_OC and 
					 (RECEPCION_PRODUCTO_det.NRO_reg = :v_recep or :v_recep = 0) and
					 RECEPCION_PRODUCTO_DET.CODIGO = Pedidos_det.Codigo), 0) AS Cant_recibida_bonif,

			isnull((select sum(cantidad) from compras with (nolock), COMPRAS_DET with (nolock), tipo_documentos with (nolock)
			 where compras.nro_reg = compras_det.nro_reg and
					 COMPRAS.NRO_PEDIDO = Pedidos.Nro_OC  and
					 COMPRAS.tipo_documen = tipo_documentos.tipo_documen  and
					 tipo_documentos.tipo_nc = 0 and
					 (compras.NRO_recepcion = :v_recep or :v_recep = 0)and 
					 COMPRAS_DET.CODIGO = Pedidos_det.Codigo), 0) AS Cant_facturada,

			isnull((select sum(cantidad) from compras with (nolock), COMPRAS_DET with (nolock), tipo_documentos with (nolock)
			 where compras.nro_reg = compras_det.nro_reg and
					 COMPRAS.NRO_PEDIDO = Pedidos.Nro_OC  and
					 COMPRAS.tipo_documen = tipo_documentos.tipo_documen  and
					 tipo_documentos.tipo_nc = 1 and
					 (compras.NRO_recepcion = :v_recep or :v_recep = 0)and 
					 COMPRAS_DET.CODIGO = Pedidos_det.Codigo), 0) AS Cant_ajustada,

			isnull((select sum(CANT_BONIF) from compras with (nolock), COMPRAS_DET with (nolock)
			 where compras.nro_reg = compras_det.nro_reg and
					 (compras.NRO_recepcion = :v_recep or :v_recep = 0)and 
					COMPRAS_DET.NRO_PEDIDO = Pedidos.Nro_OC and 
					 COMPRAS_DET.CODIGO = Pedidos_det.Codigo), 0) AS Cant_bonif,


			isnull((select sum((cantidad + cant_desc) * precio) from compras with (nolock), COMPRAS_DET with (nolock)
			 where compras.nro_reg = compras_det.nro_reg and
					 (compras.NRO_recepcion = :v_recep or :v_recep = 0)and 
					 COMPRAS.NRO_PEDIDO = Pedidos.Nro_OC and 
					 COMPRAS_DET.CODIGO = Pedidos_det.Codigo), 0) AS Importe_facturada,

			isnull((select sum(cant_desc * precio) from compras with (nolock), COMPRAS_DET with (nolock)
			 where compras.nro_reg = compras_det.nro_reg and
					 (compras.NRO_recepcion = :v_recep or :v_recep = 0)and 
					 COMPRAS.NRO_PEDIDO = Pedidos.Nro_OC and 
					 COMPRAS_DET.CODIGO = Pedidos_det.Codigo), 0) AS Importe_ajustado,

			isnull((select top 1 cod_deposito from compras with (nolock)
			 where COMPRAS.NRO_PEDIDO = Pedidos.Nro_OC and
					 (compras.NRO_recepcion = :v_recep or :v_recep = 0) ), 0) AS Deposito,

			oc.abierta,
         plazos.plazo,
			compras_condicion.condicion,
			par_sys.codigo_devol,
			Pedidos.Cod_proveedor,
			Proveedores.ruc,
			(select top 1 timbrado from proveedor_timbrado with (nolock) where cod_proveedor =Proveedores.cod_proveedor)  as timbrado,
			(select top 1 vto_timbrado from proveedor_timbrado with (nolock) where cod_proveedor =Proveedores.cod_proveedor)  as VTO_TIMBRADO

    FROM dbo.Pedidos with (nolock),   
         dbo.Pedidos_det with (nolock),   
         dbo.Productos with (nolock),   
         dbo.Proveedores with (nolock),
			oc with (nolock),
			oc_det with (nolock),
		   tipo_documentos with (nolock),
			plazos with (nolock),
			compras_condicion with (nolock),
			par_sys with (nolock)
   WHERE ( oc.tipo_documen = tipo_documentos.tipo_documen ) and 
			( oc.nro_oc = oc_det.nro_oc ) and
			( oc.cod_plazo = plazos.cod_plazo ) and
			( oc.cod_condicion = compras_condicion.cod_condicion ) and
			( oc_det.Nro_pedido = Pedidos.Nro_pedido ) and  
			( Pedidos.Nro_oc = Oc.Nro_oc) and
			( Pedidos.Nro_pedido = Pedidos_det.Nro_pedido ) and  
         ( Pedidos_det.Codigo = Productos.Codigo ) and  
         ( Pedidos.Cod_proveedor = Proveedores.Cod_proveedor ) and  
         ( Pedidos.Nro_OC = :V_NroReg ) 
Group by Pedidos.Nro_pedido,
			Pedidos.Nro_oc,
			Pedidos.fecha,
			Proveedores.Nombre_proveedor,
			Pedidos.Observacion,
			Pedidos_det.Codigo,
			Productos.Descripcion_producto,
			Productos.Codigo_barras,
			Productos.Tipo_fiscal,
			Productos.imp_imp,
			Pedidos_det.imp_imp,
			Oc.Abierta,
			Tipo_documentos.Calc_imp,
			Tipo_documentos.Tributo_unico,
			Pedidos_det.Precio,
			Pedidos_det.Observacion,
			Productos.Unidad,
			Plazos.Plazo,
			compras_condicion.Condicion,
			Par_sys.Codigo_devol,
			Pedidos.Cod_proveedor,
			Proveedores.Ruc,
			Proveedores.Cod_Proveedor


union all

  SELECT 0,   
			compras.Nro_pedido,   
         compras.Fecha,  
         Proveedores.Nombre_proveedor,   
         compras.Observacion,   
         compras_det.Codigo,   
         Productos.Descripcion_producto,   
         Productos.codigo_barras,   
         compras_det.tipo_fiscal,
			Productos.imp_imp,
         compras_det.imp_imp, 
         0 as cantidad,   
      	compras_det.precio as det_Precio,

         '',   
         Productos.Unidad,
			'' as fecha_rec,

			0 AS Cant_recibida,
			0 AS Cant_recibida_bonif,

			sum(compras_det.cantidad) AS Cant_facturada,
			0,

			sum(compras_det.CANT_BONIF) AS Cant_bonif,


			sum(compras_det.cantidad * compras_det.precio) AS Importe_facturada,
			0,
			isnull(max(compras.cod_deposito),0) AS Deposito,

			oc.abierta,
         '' as plazo,
			'' as condicion,
			par_sys.codigo_devol,
			compras.Cod_proveedor,
			Proveedores.ruc,
			compras.timbrado_prov as timbrado,
			compras.VTO_TIMBRADO as vto_timbrado
    FROM compras with (nolock),   
         compras_det with (nolock),   
         Productos with (nolock),   
         Proveedores with (nolock),
			oc with (nolock),
		   tipo_documentos with (nolock),
			par_sys with (nolock)
   WHERE ( oc.tipo_documen = tipo_documentos.tipo_documen ) and 
			( compras.Nro_reg = compras_det.Nro_reg ) and  
			( compras.Nro_pedido = oc.Nro_oc ) and  
         ( compras_det.Codigo = Productos.Codigo ) and  
         ( compras_det.Codigo = (select codigo_devol from  par_sys) ) and  
         ( compras.Cod_proveedor = Proveedores.Cod_proveedor ) and  
         ( compras.Nro_pedido = :V_NroReg ) and 
			( compras.NRO_recepcion = :v_recep or :v_recep = 0 )
group by compras.Nro_pedido,   
         compras.Fecha,  
         Proveedores.Nombre_proveedor,   
			compras.Cod_proveedor,
         compras.Observacion,   
         compras_det.Codigo,   
         Productos.Descripcion_producto,   
         Productos.codigo_barras,   
         compras_det.tipo_fiscal,
			Productos.imp_imp,
         compras_det.imp_imp, 
      	compras_det.precio,
         Productos.Unidad,
			oc.abierta,
			par_sys.codigo_devol, 
			Proveedores.ruc,
			Proveedores.timbrado,
			Proveedores.VTO_TIMBRADO,
			compras.timbrado_prov,
			compras.VTO_TIMBRADO
