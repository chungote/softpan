 column=(type=datetime updatewhereclause=yes name=pedidos_fecha dbname="PEDIDOS.Fecha" )
 column=(type=char(60) updatewhereclause=yes name=proveedores_nombre_proveedor dbname="PROVEEDORES.Nombre_proveedor" )
